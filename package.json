{
  "name": "vibetools",
  "version": "0.1.0",
  "private": false,
  "description": "CLI for managing AI agent skills/commands via a git-backed vibetools repo.",
  "license": "UNLICENSED",
  "bin": {
    "vibetools": "dist/cli.mjs"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsdown",
    "lint": "oxlint --type-aware",
    "format": "oxfmt",
    "typecheck": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "ultracite check",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "diff": "^8.0.3",
    "picomatch": "^4.0.3",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.17.31",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "oxlint-tsgolint": "^0.11.4",
    "tsdown": "^0.20.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "ultracite": "7.1.4",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.26.0"
}
